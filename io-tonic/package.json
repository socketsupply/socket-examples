{
  "name": "@socketsupply/example-io",
  "type": "module",
  "version": "0.0.0",
  "description": "IO example",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && ssc build -r .",
    "build": "mkdir -p public && rm -rf dist/* public/* && cp src/index.html public && cp src/style.css public && npm run build-js",
    "build-js": "mkdir -p ./public && esbuild src/index.js --bundle --outfile=public/bundle.js",
    "build-tests": "esbuild test/test.js --bundle --outfile=public/test.js",
    "test": "npm run build && npm run build-tests && ssc build --test=test.js --headless -r -o . | tap-arc"
  },
  "dependencies": {
    "@socketsupply/io": "github:socketsupply/io",
    "@socketsupply/tonic": "^14.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/socketsupply/example-io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/socketsupply/example-io/issues"
  },
  "homepage": "https://github.com/socketsupply/example-io#readme",
  "devDependencies": {
    "@socketsupply/ssc-test": "github:socketsupply/ssc-test",
    "@socketsupply/test-dom": "^0.5.0",
    "esbuild": "^0.16.4",
    "tap-arc": "^0.3.5",
    "tapzero": "^0.6.1"
  }
}
